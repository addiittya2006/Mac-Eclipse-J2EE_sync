<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>JDBCTest</display-name>
  <context-param>
    <param-name>jdbc_driver</param-name>
    <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>jdbc_url</param-name>
    <param-value>jdbc:mysql://localhost/emp</param-value>
  </context-param>
  <context-param>
    <param-name>jdbc_user</param-name>
    <param-value>aditya</param-value>
  </context-param>
  <context-param>
    <param-name>jdbc_pass</param-name>
    <param-value></param-value>
  </context-param>
  <servlet>
    <servlet-name>s1</servlet-name>
    <servlet-class>com.addiittya.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s1</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>s2</servlet-name>
    <servlet-class>com.addiittya.WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s2</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>